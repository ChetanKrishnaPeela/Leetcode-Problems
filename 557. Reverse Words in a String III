class Solution 
{
    public String reverseWords(String s) 
    {
        String s2[] = s.split(" ");
        String ans[] = new String[s2.length];
        int j=0;
        for(String i:s2)
        {
            StringBuilder temp= new StringBuilder(i);
            temp.reverse();
            ans[j++] = temp.toString();
        }
        return String.join(" ",ans);
    }
}
