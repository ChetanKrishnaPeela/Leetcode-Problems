class Solution 
{
    public int fib(int n) 
    {
        // Using reccursion
        // if(n < 2){return n;}
        // return fib(n-1) + fib(n-2);

        int num1 = 1,num2 = 0, ans = 0;
        for(int i=0; i < n;i++)
        {
            ans = num1 + num2;
            num1 = num2;
            num2 = ans;
        }
        return ans;
    }
}
