class Solution 
{
    public List<Integer> findDisappearedNumbers(int[] nums) 
    {
        int n=nums.length;
        int arr[] = new int[n+1];
        for(int i:nums){if(arr[i] == 0) arr[i] = i;}

        List<Integer> ans = new ArrayList<>();
        for(int i=1; i<=n; i++){if(arr[i] == 0) ans.add(i);}
        return ans;
    }
}
