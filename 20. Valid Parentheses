class Solution 
{
    public boolean isValid(String s) 
    {
        int n = s.length();
        Stack<Character> s2 = new Stack<>();
        for(int i=0; i<n; i++)
        {
            if((s2.size() == 0) && ((s.charAt(i) == ')') || (s.charAt(i) == '}') || (s.charAt(i) == ']'))) return false;
            else if(s2.size() == 0) s2.push(s.charAt(i));
            else if((s2.peek() == '(') && (s.charAt(i) == ')')|| (s2.peek() == '{') && (s.charAt(i) == '}')|| (s2.peek() == '[') && (s.charAt(i) == ']'))
            {
                s2.pop();
            }
            else s2.push(s.charAt(i));
        }
        return s2.isEmpty();
    }
}
